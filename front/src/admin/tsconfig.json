{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@admin/*": [
        "./src/*"
      ],

      "@modules/*": [
        "../modules/src/*"
      ],
    },
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "../../node_modules/*",
    "../../node_modules",
    "node_modules/*",
    "node_modules"
  ]
}

{
  "compilerOptions": {
    "strict": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "strictPropertyInitialization": true,
    "alwaysStrict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "noImplicitAny": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "inlineSources": true,
    "incremental": false,
    "removeComments": false,
    "allowJs": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "noEmit": false,
    "moduleResolution": "node",
    "module": "ESNext",
    "target": "ESNext",
    "jsx": "react-jsx",
    "plugins": [
      {
        "name": "eslint-plugin-import-access",
        "jsdoc": {
          "indexLoophole": true,
          "filenameLoophole": true
        }
      },
    ]
  },
  "exclude": [
    "dist/*",
    "dist",
    "**/openapi/**/*",
    "**/openapi/model",
    "openapi/**/*",
    "src/openapi/**/*",
    "node_modules/*",
    "node_modules"
  ]
}

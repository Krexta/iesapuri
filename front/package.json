{
  "name": "iesapuri",
  "private": true,
  "scripts": {
    "lint-ts": "eslint --ext js --ext jsx --ext ts --ext tsx ./ --ignore-path .gitignore",
    "new-fc": "hygen new fc",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "workspaces": [
    "src/*"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,vue}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.scss": [
      "prettier --parser scss --write",
      "git add"
    ]
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.50.1",
    "unstated-next": "^1.1.0"
  },
